  },
  "homepage": "https://github.com/dioxair/grep-clone#readme",
  "dependencies": {
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.1.0",
    "rollup": "^4.20.0",
    "rollup-plugin-combine": "^2.1.1",
    "rollup-plugin-match": "^1.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
